{
  "name": "create-mf-app",
  "version": "1.0.12",
  "description": "Create Module Federation apps",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/create-mf-app.js",
  "scripts": {
    "start": "node bin/create-mf-app.js",
    "lint": "eslint . --ext .js,.ts --fix",
    "build": "tsc",
    "prebuild": "yarn del dist",
    "postbuild": "cpy templates dist --parents",
    "prepare": "husky install"
  },
  "author": "Jack Herrington",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
    "ncp": "^2.0.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^8.1.3",
    "@types/ncp": "^2.0.5",
    "@types/node": "^16.11.11",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint"
    ]
  }
}
